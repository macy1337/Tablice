#include <iostream>
using namespace std;

void generuj(int T[], int n)
{
    T[0] = 1;
    T[1] = 1;
    for (int i = 2; i <= n; i++)
    {
        T[i] = T[i-1] + T[i-2];
    }
    
}

void pokaz(int T[], int n)
{
    for (int i = 0; i < n; i++)
    {
        cout << T[i] << " ";
    }
}

int main()
{
    int T[1000];
    int n;
    cout << "Podaj n: ";
    cin >> n;
    generuj(T,n);
    pokaz(T,n);
}
