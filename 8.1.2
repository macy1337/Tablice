#include <iostream>
using namespace std;

void generuj(int T[])
{
    for(int i = 0; i < 10; i++)
    {
        cout << "T[" << i << "]=";
        cin >> T[i];
    }
}

void pokaz(int T[])
{
    for(int i = 0; i < 10; i++)
    {
        cout << "T[" << 9 - i << "]=" << T[9 - i] << " ";
    }
}

int main()
{
    int T[10];
    generuj(T);
    pokaz(T);
    return 0;
}
