#include <iostream>
using namespace std;

bool parz(string liczba)
{
    bool jest = 1;
    int t;
    for (int i = 0; i < liczba.size(); i++)
    {
        t = liczba[i] - 48;
        if (t % 2 != 0) jest = 0;
    }
    return jest;
}

int main()
{
    string T[3];
    for (int i = 0; i < 3; i++)
    {
        cout << "T[" << i << "]=";
        cin >> T[i];
    }

    for (int i = 0; i < 3; i++)
        if (parz(T[i])) cout << T[i] << " ";
}
