#include <iostream>
#include <string>
using namespace std;

void generuj(int T[], int n)
{
    for (int i = 0; i < n; i++)
    {
        cout << "T[" << i << "]=";
        cin >> T[i];
    }
}

bool suma(int liczba)
{
    string str = to_string(liczba);
    int x = str[0] - 48;
    if (x == 1)
    {
        for (int i = 1; i < str.size(); i++)
        {
            int y = str[i] - 48;
            if (y != 0) return false;
        }
        return true;
    }
    return false;
}

void pokaz(int T[], int n)
{
    for (int i = 0; i < n; i++)
    {
        if (suma(T[i]) == true)
        {
        cout << "T[" << i << "]=" << T[i] << " ";
        }
    }
}

int main()
{
    int T[1000];
    int n;
    cout << "Podaj n: ";
    cin >> n;
    generuj(T, n);
    pokaz(T, n);
    return 0;
}
