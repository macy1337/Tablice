#include <iostream>
#include <string>
using namespace std;

int losuj(int a, int b)
{
    return (rand() % (b - a + 1)) + a;
}

void generuj(int T[], int n)
{
    for (int i = 0; i < n; i++)
    {
        T[i] = losuj(1,10);
    }
}

void pokaz(int T[], int n)
{
    for (int i = 1; i * i < n; i++)
    {
        cout << "T[" << i * i << "]=" << T[i * i] << " ";
    }
}

int main()
{
    int T[1000];
    int n;
    cout << "Podaj n: ";
    cin >> n;
    srand(time(NULL));
    generuj(T, n);
    pokaz(T, n);
    return 0;
}
