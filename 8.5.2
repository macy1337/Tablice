#include <iostream>
using namespace std;

int los(int a, int b)
{
    return (rand() % b - a + 1) + a;
}

void generuj(int T[], int n)
{
    for(int i = 0; i < n; i++)
    {
        T[i] = los(1,10);
    }
}

void pokaz(int T[], int n)
{
    for(int i = 0; i * i < n; i++)
    {
        int index = i * i;
        cout << "T[" << index << "]=" << T[index] << " ";
    }
}

int main()
{
    int T[1000];
    int n;
    cout << "Podaj n: ";
    cin >> n;
    srand(time(NULL));
    generuj(T, n);
    pokaz(T, n);
}
